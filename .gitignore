# Ignore directories generated by Composer
/drush/contrib/
/console/
/vendor/
/console/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
!/web/libraries/

# Ignore sensitive information
/web/sites/*/settings.*
/web/sites/*/settings.local.php
/web/sites/*/services.yml

# Ignore Drupal's file directory
/web/sites/*/files/

# Ignore SimpleTest multi-site environment.
/web/sites/simpletest

# Ignore files generated by Visual Code and PHPSTORM
/.vscode
/.idea

# Ignore .env files as they are personal
/.env

# Ignore .DS_STORE files generated by MacOS
.DS_STORE

# Ignore content sync folder setting by content_sync module
/config/content/

# Ignore translation folder
/web/sites/default/translations

# Ignore private folder
/private

# Ignore .history folder
/.history

# Ignore other folders
/backup

# Ignore docksal-local configuration
/.docksal/docksal-local.env

# Other locals
/blocks
